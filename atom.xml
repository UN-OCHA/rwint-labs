---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>ReliefWeb Labs RSS</title>
 <link href="https://labs.reliefweb.int/atom.xml" rel="self"/>
 <link href="https://labs.reliefweb.int/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://labs.reliefweb.int/</id>
 <author>
   <name>ReliefWeb</name>
   <email>labs@reliefweb.int</email>
 </author>

 {% assign sortedProjects = site.projects | sort: 'weight' | reverse %}
 {% for post in sortedProjects %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://labs.reliefweb.int{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://labs.reliefweb.int{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
